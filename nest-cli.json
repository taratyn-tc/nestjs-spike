{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "webapp_main",
  "compilerOptions": {
    "deleteOutDir": true
  },
  "plugins": [
    {
      "collection": "@nestjs/schematics",
      "sourceRoot": "src",
      "compilerOptions": {
        "plugins": [
          "@nestjs/swagger"
        ]
      }
    }
  ]
}
